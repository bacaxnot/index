{
  "name": "bacaxnot",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "sst dev turbo dev --verbose",
    "deploy:prod": "sst deploy --stage=prod",
    "undeploy:prod": "sst remove --stage=prod",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@bxn/tsconfig": "workspace:*",
    "@bxn/eslint-config": "workspace:*",
    "prettier": "^3.1.1",
    "turbo": "latest"
  },
  "packageManager": "pnpm@8.9.0",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "sst": "^3.0.1"
  }
}